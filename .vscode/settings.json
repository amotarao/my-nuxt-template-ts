{
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true,
  },
  "eslint.validate": [
    "typescript",
    "javascript",
    "vue"
  ],
  "eslint.packageManager": "yarn",
  "stylelint.validate": [
    "css",
    "scss",
    "vue"
  ],
  "stylelint.packageManager": "yarn"
}
